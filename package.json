    "@headlessui/react": "^1.7.18",
    "@hookform/resolvers": "^3.3.4",
    "@lexical/react": "^0.14.2",
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-query": "^5.28.8",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "apexcharts": "^3.50.0",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lexical": "^0.14.2",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-datepicker": "^6.6.0",
