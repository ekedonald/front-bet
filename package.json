{
  "name": "portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">1.19.1"
  },
  "scripts": {
    "dev": "vite",
    "check-typing": "tsc",
    "build": "yarn run check-typing && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:q": "vitest --silent --run",
    "test:ui": "vitest --ui",
    "lint": "eslint --fix --ext ts,tsx ./src --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
