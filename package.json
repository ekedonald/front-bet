    "sonner": "^1.4.41",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/rollup": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@tanstack/react-query-devtools": "^5.28.8",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-datepicker": "^6.2.0",
